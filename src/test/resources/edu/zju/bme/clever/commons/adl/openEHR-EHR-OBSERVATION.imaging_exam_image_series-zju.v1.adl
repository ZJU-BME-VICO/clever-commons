                                            }
                                            ELEMENT[at0008] occurrences matches {0..1} matches {	--	Key image
                                                value matches {
                                                    DV_BOOLEAN matches {*}
                                                }
                                            }
                                            ELEMENT[at0020] occurrences matches {0..1} matches {	--	Image identifier
                                                value matches {
